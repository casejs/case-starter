{
  "name": "case-server",
  "version": "0.0.0",
  "description": "description",
  "author": "CASE",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js",
    "seed": "ts-node database/seeders/seeder.ts && cp -R assets/dummy/* public/storage/",
    "case:resourceBack": "schematics @case-app/schematics/collection.json:createResourceServer && npm run seed"
  },
  "dependencies": {
    "@bugsnag/js": "^7.17.0",
    "@case-app/nest-library": "^0.3.0",
    "@nestjs/common": "^9.0.3",
    "@nestjs/core": "^9.0.3",
    "@nestjs/platform-express": "^9.0.3",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/typeorm": "^9.0.0",
    "bluebird": "^3.7.2",
    "class-transformer": ">=0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "crypto-js": "^4.1.1",
    "docxtemplater": "^3.31.2",
    "exceljs": "^4.3.0",
    "express": "^4.18.1",
    "handlebars": "^4.7.7",
    "http-status-codes": "^2.2.0",
    "i": "^0.3.7",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "moment-business-days": "^1.2.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "npm": "^8.14.0",
    "pizzip": "^3.1.1",
    "puppeteer": "^15.4.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.6",
    "sharp": "^0.30.7",
    "slugify": "^1.6.5",
    "typeorm": "^0.3.7",
    "typescript": "^4.7.4",
    "uniqid": "^5.4.0"
  },
  "devDependencies": {
    "@nestjs/testing": "9.0.3",
    "@types/cron": "^2.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^6.6.8",
    "@types/jest": "^28.1.6",
    "@types/mailgun-js": "^0.22.12",
    "@types/node": "^18.0.6",
    "@types/supertest": "^2.0.12",
    "faker": "^5.5.3",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "tslint": "5.20.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
