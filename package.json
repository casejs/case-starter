{
  "name": "case-starter",
  "version": "0.0.1",
  "description": "A super cool CASE application",
  "author": "A great CASE developer",
  "license": "MIT",
  "scripts": {
    "compile": "tsc && node node_modules/@casejs/case/server/src/main.js",
    "start": "nodemon",
    "seed": "node node_modules/@casejs/case/server/src/dynamic-entity/seed.js",
    "case:entity": "node_modules/@angular-devkit/schematics-cli/bin/schematics.js ./node_modules/@casejs/schematics/collection.json:createEntity --debug=false --name"
  },
  "dependencies": {
    "@angular-devkit/schematics-cli": "^16.1.4",
    "@casejs/case": "^2.0.19",
    "@casejs/schematics": "^2.0.3",
    "@faker-js/faker": "^8.0.2",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1"
  }
}
