{
  "name": "case-starter",
  "version": "0.0.1",
  "description": "A super cool CASE application",
  "author": "A CASE developer",
  "license": "MIT",
  "scripts": {
    "install": "cd node_modules/@casejs/case/server && npm install && npm run build",
    "start": "nodemon",
    "seed": "ts-node node_modules/@casejs/case/server/src/dynamic-entity/seed.ts",
    "case:entity": "node_modules/@angular-devkit/schematics-cli/bin/schematics.js ./node_modules/@casejs/schematics/collection.json:createEntity --debug=false --name "
  },
  "dependencies": {
    "@angular-devkit/schematics-cli": "^15.0.0",
    "@casejs/case": "^2.0.19",
    "@casejs/schematics": "^2.0.1",
    "@faker-js/faker": "^8.0.2",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1"
  }
}
